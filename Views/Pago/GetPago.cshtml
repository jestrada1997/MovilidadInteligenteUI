@model MovilidadInteligenteUI.Models.Pago

@{
    ViewData["Title"] = "Detalle del Pago";
}

<br />
<body>
    <br />
    <div class="container">
        <div class="card">
            <div class="card-body">

                <div>
                    <h4>Detalle del Pago</h4>
                    <hr />
                    <dl class="row">
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.idPago)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.idPago)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.idUsuario)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.idUsuario)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.idUnidad)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.idUnidad)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.monto)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.monto)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.fechaPago)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.fechaPago)
                        </dd>
                        <dt class="col-sm-2">
                            @Html.DisplayNameFor(model => model.estado)
                        </dt>
                        <dd class="col-sm-10">
                            @Html.DisplayFor(model => model.estado)
                        </dd>
                    </dl>
                </div>
                <div class="form-group">
                    <a asp-action="Pagos" class="btn btn-primary">Regresar</a>
                </div>
            </div>
        </div>
    </div>
</body>